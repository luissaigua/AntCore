                                    <br />
                                    <label class="checkbox-inline">
                                        <input type="checkbox" id="cb_trabajosViaEdit" value="option1">Trabajos Vía
                                    </label>
                                </div>*@
                            </div>
                                <div class="col-lg-12">
                                   <br />
                                    <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                        Curva Existente*
                                        @Html.DropDownList("ddl_curvaExistenteEdit", (List<SelectListItem>)ViewBag.ObtenerlistadatosCurvaExistente, new { @class = "form-control" })
                                    </div>
