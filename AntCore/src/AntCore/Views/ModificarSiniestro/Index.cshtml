                                                @Html.TextBox("tb_modvehinvEdit", null, new { @class = "form-control", @placeholder = "Modelo del VHL", maxlength = "60" })
                                            </div>
                                            <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
