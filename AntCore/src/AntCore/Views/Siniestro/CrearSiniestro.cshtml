                                    <select id="ddl_ciudad" class="form-control" style="font-size:11px" disabled></select>
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-6">
                                    Parroquia*
                                    @Html.DropDownList("ddl_parroquia", (List<SelectListItem>)ViewBag.ObtenerlistaParroquias, new { @class = "form-control", @placeholder = "PROVINCIAS", style = "font-size:11px", disabled = "true" })
                                </div>
                            </div>
                            <div class="col-lg-12" id="divDistritos" style="display:none">
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-6">
                                    Distrito*
                                    @Html.DropDownList("ddl_distritoSiniestro", (List<SelectListItem>)ViewBag.ObtenerlistaTodosDistritos, new { @class = "form-control", @placeholder = "PROVINCIAS", style = "font-size:11px" , disabled = "true" })
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-6">
                                    Circuito*
                                    @Html.DropDownList("ddl_circuito", (List<SelectListItem>)ViewBag.ObtenerlistaTodosCiruitos, new { @class = "form-control", @placeholder = "PROVINCIAS", style = "font-size:11px" , disabled = "true" })

                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-12">
                                    Dirección Siniestro*
                                    @Html.TextBoxFor(Model => Model.dirsin, null, new { id = "tbdireccionSiniestro", @class = "form-control", maxlength = "150", @placeholder = "Dirección siniestro", onkeypress = "aMayusculas(this.value,this.id)" })
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3" id="divTipoZona" style="display:none">
                                    Zona*
                                    @Html.DropDownList("ddl_tipo_zona", (List<SelectListItem>)ViewBag.tipoZonaLista, new { @class = "form-control", @placeholder = "0", disabled = "true" })
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <br />
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-1" style="display:none">
                                    #Fallecidos
