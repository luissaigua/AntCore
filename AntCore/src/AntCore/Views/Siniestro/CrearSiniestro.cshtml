                            <div class="col-lg-12">
                                
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-2">
                                    Fecha*
                                    @Html.TextBoxFor(Model => Model.fecsin, null, new { id = "tbfechaSiniestro", @class = "form-control", @placeholder = "aaa-mm-dd", @title = "Tooltip on top" })
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-2">
                                    Hora*
                                    @Html.TextBoxFor(Model => Model.horsin, null, new { id = "tbHoraSiniestro", @class = "form-control", @placeholder = "00:00", onkeypress = "valida_hora(value)" })
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-2">
                                    Latitud(Y)*
                                   @Html.TextBoxFor(Model => Model.latsin, null, new { id = "tbLatitudSiniestro", @class = "form-control", placeholder = "0", maxlength = "13",title = "Rango de -5.014379 a 1.448413 Ejemplo Y=-0.202363", onkeypress = "return NumCheck(event, this)" })
                                    
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">

                                    Longitud(X)*
                                    <div class="form-group input-group">
                                        
                                        @Html.TextBoxFor(Model => Model.lonsin, null, new { id = "tbLonguitudSiniestro", @class = "form-control", maxlength = "13", @placeholder = "0",  title = "Rango de -91.662058 a -75.188745 Ejemplo  X=-78.495769", onkeypress = "return NumCheck(event, this)" })
