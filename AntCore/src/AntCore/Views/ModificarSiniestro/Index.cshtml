                                                @Html.DropDownList("ddl_vehiculo_identificadoEdit", (List<SelectListItem>)ViewBag.ObtenerlistadatosValoresSiNo, new { @class = "form-control" })
                                            </div>
                                            <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
