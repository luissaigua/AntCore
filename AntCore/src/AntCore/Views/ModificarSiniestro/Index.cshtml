                                                @Html.TextBox("tb_anivehinvEdit", null, new { @class = "form-control", @placeholder = "Año VHL" , maxlength = "4" })
                                            </div>
                                            <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
