@model Modelo.Entity.Departamento
@{
    //ViewBag.Title = "Nuevo Departamento";
    Layout = null;
}


<form asp-action="Create" class="form-control-static">

    <h4>Informacion Departamento:</h4>

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-lg-6">
            <label asp-for="PD000" class="control-label"></label>

            <input asp-for="PD000" class="form-control" />
            <span asp-validation-for="PD000" class="text-danger" />

        </div>

        <div class="col-lg-12">
            <hr />
            <input type="submit" value="Guardar" class="btn btn-primary pull-right" />

        </div>
    </div>

</form>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
