                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-1" style="display:none">
                                    #Lesionados
                                    @Html.TextBoxFor(Model => Model.numlessin, null, new { id = "tbnumlessin", @class = "form-control", @placeholder = "0" })
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Condición Atmosférica*
                                    @Html.DropDownList("ddl_condAtmosferica", (List<SelectListItem>)ViewBag.ObtenerListadoCondicionAtmosferica, new { @class = "form-control", @placeholder = "0" })
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Condición de la Vía*
                                    @Html.DropDownList("ddl_condVia", (List<SelectListItem>)ViewBag.ObtenerlistadatosCondicionVia, new { @class = "form-control", @placeholder = "0" })
                                </div>


                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Tipo de vía*
                                    @Html.DropDownList("ddl_tipoVia", (List<SelectListItem>)ViewBag.ObtenerlistadatosTipoVia, new { @class = "form-control", @placeholder = "0" })
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-2">
                                    #Carriles*
                                    @Html.DropDownList("ddl_numCarriles", (List<SelectListItem>)ViewBag.ObtenerlistadatosNumeroCarriles, new { @class = "form-control", @placeholder = "0" })
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Material de Superficie*
                                    @Html.DropDownList("ddl_materialSuperficie", (List<SelectListItem>)ViewBag.ObtenerlistadatosMaterialSuperficieVia, new { @class = "form-control", @placeholder = "0" })
                                </div>
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-2">
                                    Límite de velocidad*
                                    @Html.DropDownList("ddl_limVelocidad", (List<SelectListItem>)ViewBag.ObtenerlistadatosLimiteVelocidad, new { @class = "form-control", @placeholder = "0" })
                                </div>


                            </div>

                            <div class="col-lg-12">
                                <br />
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-2">
                                    Trabajos en la vía*
                                    @Html.DropDownList("ddl_trabajosVia", (List<SelectListItem>)ViewBag.ObtenerlistadatosValoresSiNo, new { @class = "form-control", @placeholder = "0" })
                                    <label class="checkbox-inline" style="display:none">
                                        <input type="checkbox" id="cb_trabajosVia" value="option1">Trabajos Vía
                                    </label>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Lugar de la vía*
                                    @Html.DropDownList("ddl_lugarVia", (List<SelectListItem>)ViewBag.ObtenerlistadatosLugarVia, new { @class = "form-control", @placeholder = "0" , style = "font-size:11px" })
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Control Intersección*
                                    @Html.DropDownList("ddl_controlInterseccion", (List<SelectListItem>)ViewBag.ObtenerlistadatosControlInterseccion, new { @class = "form-control", @placeholder = "0", disabled = "true" })
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Señalización Existente*
                                    @Html.DropDownList("ddl_senializacionExistente", (List<SelectListItem>)ViewBag.ObtenerlistadatosSenialitica, new { @class = "form-control" })
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Curva Existente*
                                    @Html.DropDownList("ddl_curvaExistente", (List<SelectListItem>)ViewBag.ObtenerlistadatosCurvaExistente, new { @class = "form-control" , disabled="true"})
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Obstáculos de la Vía*
                                    @Html.DropDownList("ddl_obstVia", (List<SelectListItem>)ViewBag.ObtenerlistadatosObstaculoVia, new { @class = "form-control", @placeholder = "0" })
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <br />
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-12">
                                    Causa Probable Siniestro*
                                    @Html.DropDownList("ddl_causaProbableSiniestro", (List<SelectListItem>)ViewBag.ObtenerlistaCausaProbableSiniestros, new { @class = "form-control", style = "font-size:11px" })
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <br />
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-6">
                                    Tipo de Siniestro*
                                    @Html.DropDownList("ddl_tipoSiniestro", (List<SelectListItem>)ViewBag.ObtenerlistatipoSiniestros, new { @class = "form-control" })
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-4" id="divLuzArtificial">
                                    Luz Artificial*
                                    @Html.DropDownList("ddl_luzArtificial", (List<SelectListItem>)ViewBag.ObtenerlistadatosLuzArtificial, new { @class = "form-control", @placeholder = "0", disabled = "true" })
                                </div>

                                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                    Campos Obligatorios (*)<br />
                                    <a class="btn btn-primary" id="btnGuardaSiniestro">Ingresar Siniestro  </a>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <br />
                                <div ID="panelMensajeSiniestro" Class="active smaller" style="display:none">
                                    <div id="divMensajeSiniestro" class="alert alert-block alert-danger fade in smaller">
                                        <strong><label ID="mensajeStrongLabelSiniestro"> </label></strong>
                                        <label ID="mensajeLabelSiniestro"></label>
                                    </div>
                                </div>
                            </div>
                           
                        
                        </div>
                    </div>
