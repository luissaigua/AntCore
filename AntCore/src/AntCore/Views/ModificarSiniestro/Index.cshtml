                                                @Html.TextBox("tb_marvehinvEdit", null, new { @class = "form-control", @placeholder = "Marca Comercial VHL", maxlength = "60" })
                                            </div>
                                            <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3">
