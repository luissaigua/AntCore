                                                Tipo Servicio*
                                                @Html.DropDownList("ddl_tipo_servicio_vhlEdit", (List<SelectListItem>)ViewBag.ObtenerlistaServiciosVehiculos, new { @class = "form-control" })
                                            </div>
                                            <div class="col-xs-3 col-sm-3 col-md-3 col-lg-4">
