                                        Obstáculos de la Vía*
                                        @Html.DropDownList("ddl_obstViaEdit", (List<SelectListItem>)ViewBag.ObtenerlistadatosObstaculoVia, new { @class = "form-control", @placeholder = "0" })
                                    </div>
                                    <div class="col-xs-12 col-sm-4 col-md-3 col-lg-4">
                                        Tipo de Siniestro*
                                        @Html.DropDownList("ddl_tipoSiniestroEdit", (List<SelectListItem>)ViewBag.ObtenerlistatipoSiniestros, new { @class = "form-control" })
                                    </div>


