                                                                                <td>SI</td>
                                                                            }

                                                                            else
                                                                            {
                                                                                <td>NO</td>
                                                                            }
                                                                            <td>@objSin.chavehinv</td>
                                                                            <td>@objSin.marvehinv</td>
                                                                            <td>@objSin.modvehinv</td>
                                                                            <td>@objSin.anivehinv</td>
                                                                            <td>@objSin.cilvehinv</td>

                                                                            @if (@objSin.segprivehinv == true)
                                                                            {

                                                                                <td>SI</td>
                                                                            }

                                                                            else
                                                                            {
                                                                                <td>NO</td>
                                                                            }
                                                                            <td>@objSin.matpelvehinv</td>

