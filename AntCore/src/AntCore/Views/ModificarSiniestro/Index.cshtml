                                                @Html.DropDownList("ddl_tipo_vhlEdit", (List<SelectListItem>)ViewBag.ObtenerListaTipoVehiculo, new { @class = "form-control" })
                                            </div>
                                            <div class="col-xs-3 col-sm-3 col-md-3 col-lg-4">
