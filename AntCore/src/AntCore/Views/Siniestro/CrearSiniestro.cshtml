                                            </div>
                                            <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2">
                                                Cilindraje vehículo*
                                                @Html.TextBoxFor(Model => Model.cilvehinv, null, new { id = "tb_cilvehinv", @class = "form-control", @placeholder = "Cilindraje VHL", maxlength = "10" })
                                            </div>
                                            
                                        </div>
                                        <div class="col-lg-12"  id="divTranspMaterialPeligroso" style="display:none">
