                                                @Html.DropDownList("ddl_vhl_materialPeligroso", (List<SelectListItem>)ViewBag.ObtenerlistadatosTransporteMaterialPeligroso, new { @class = "form-control" })
                                                @Html.TextBoxFor(Model => Model.matpelvehinv, null, new { id = "tb_matpelvehinv", @class = "form-control", @placeholder = "Cilindraje VHL" })
                                            </div>
                                            <div class="col-xs-3 col-sm-3 col-md-3 col-lg-4">
